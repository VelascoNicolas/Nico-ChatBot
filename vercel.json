{

    "version": 2,
  
    "builds": [
  
        {
  
            "src": "dist/main.js",
  
            "use": "@vercel/node"
  
        }
  
    ],
  
    "routes": [
  
        {
  
            "src": "/api/(.*)",
  
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
  
            "dest": "/dist/main.js"
  
        }
  
    ]
  
  }
  